{
  "name": "@uniformdev/component-starter-kit",
  "version": "6.0.0",
  "private": true,
  "engines": {
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm",
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "run-s pull:dex pull:locales next:dev",
    "build": "run-s pull:dex next:build",
    "start": "next start",
    "dev:watch": "cross-env WATCH=true run-s pull:dex next:dev",
    "next:dev": "next dev",
    "next:build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "init": "run-s uniform:push uniform:publish",
    "component:extract": "csk-components extract",
    "component:scaffold": "csk-cli scaffold",
    "pull:dex": "design-extensions-tools pull",
    "push:dex": "design-extensions-tools push",
    "pull:locales": "csk-cli pull -l",
    "pull:content": "uniform sync pull",
    "push:content": "uniform sync push",
    "pull:content:dev": "uniform sync pull --config ./uniform.config.dev.ts",
    "push:content:dev": "uniform sync push --config ./uniform.config.dev.ts",
    "uniform:pull": "run-s pull:dex pull:content",
    "uniform:push": "run-s push:dex push:content",
    "uniform:publish": "uniform context manifest publish"
  },
  "dependencies": {
    "@uniformdev/canvas-next-rsc": "^20.3.1",
    "@uniformdev/csk-components": "^6.0.0",
    "@uniformdev/design-extensions-tools": "^6.0.0",
    "next": "^15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@uniformdev/cli": "^20.3.1",
    "@uniformdev/csk-cli": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}